<% layout('layout/dashboard') -%>
<h1>Dashoard</h1>
<div class="recent-orders">
  <h2>All Users</h2>
  <table>
    <thead>
      <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email</th>
        <th>Username</th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      <%users.forEach( user =>{%>
      <tr>
        <td><%=user.firstName%></td>
        <td><%=user.lastName%></td>
        <td><%=user.email%></td>
        <td><%=user.username%></td>
        <th></th>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
